{
  "name": "falixnodes",
  "version": "4.53.0",
  "description": "Accessing Falix, all from one place",
  "main": "src/js/electron/index.js",
  "scripts": {
    "start": "electron  . > /tmp/falixnodes-desktop.log",
    "build": "electron-builder build"
  },
  "author": "KorbsStudio",
  "license": "MIT",
  "dependencies": {
    "ansi-regex": "^6.0.1",
    "custom-electron-titlebar": "^4.1.0",
    "electrino": "^0.0.0",
    "electron-context-menu": "^3.1.2",
    "electron-log": "^4.4.6",
    "electron-updater": "^4.6.5",
    "glasstron": "^0.1.1"
  },
  "devDependencies": {
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13"
  },
  "build": {
    "appId": "com.korbsstudio.falixnodes-desktop",
    "productName": "FalixNodes Desktop",
    "publish": {
      "provider": "generic",
      "url": "https://updates.korbsstudio.com/falix-software/"
    },
    "win": {
      "target": "nsis",
      "asar": "false"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "darkModeSupport": true,
      "category": "public.app-category.utilities"
    },
    "linux": {
      "target": "snap",
      "maintainer": "package.korbsstudio.falixnodes",
      "category": "Network",
      "vendor": "Korbs Studio",
      "description": "Accessing Falix, all from one place"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true,
      "shortcutName": "FalixNodes Desktop",
      "displayLanguageSelector": true,
      "oneClick": false,
      "allowElevation": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": false
    }
  }
}
