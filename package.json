{
  "name": "falixnodes",
  "version": "4.50.8",
  "description": "",
  "main": "src/js/electron/index.js",
  "scripts": {
    "start": "electron --enable-transparent-visuals --disable-gpu --disable-software-rasterizer .",
    "build": "electron-builder build",
    "rebuild": "electron-rebuild"
  },
  "author": "KorbsStudio",
  "license": "MIT",
  "dependencies": {
    "electron-log": "^4.3.5",
    "electron-updater": "^4.3.9",
    "glasstron": "^0.0.7",
    "node-abi": "^3.0.0",
    "node-static": "^0.7.11",
    "os-utils": "^0.0.14"
  },
  "devDependencies": {
    "electron": "^14.0.1",
    "electron-builder": "^22.11.7",
    "electron-nightly": "^16.0.0-nightly.20210917",
    "electron-rebuild": "^3.2.0"
  },
  "build": {
    "protocols": {
      "name": "falix",
      "schemes": [
        "falix"
      ]
    },
    "publish": {
      "provider": "generic",
      "url": "https://updates.korbsstudio.com/falix-software/"
    },
    "win": {
      "target": "nsis",
      "asar": "false"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage",
      "maintainer": "package.korbsstudio.falixnodes",
      "category": "Network",
      "vendor": "Korbs Studio",
      "description": "Accessing Falix, all from one place"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true,
      "shortcutName": "Falix Software",
      "displayLanguageSelector": true,
      "oneClick": false,
      "allowElevation": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": false
    }
  }
}
